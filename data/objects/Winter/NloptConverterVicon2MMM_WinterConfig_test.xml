      <Mapping source="STRN" target="MARKER_STRN"/>
      <Mapping source="RASI" target="MARKER_RASI"/>
      <Mapping source="LUPA" target="MARKER_LUPA"/>
      <Mapping source="LASI" target="MARKER_LASI"/>
      <Mapping source="LHIP" target="MARKER_LHIP"/>
      <Mapping source="LPSI" target="MARKER_LPSI"/>
      <Mapping source="LKNE" target="MARKER_LKNE"/>
      <Mapping source="RPSI" target="MARKER_RPSI"/>
      <Mapping source="LFRA" target="MARKER_LFRA"/>
      <Mapping source="LWPS" target="MARKER_LWPS"  weight="3.0"/>
      <Mapping source="LWTS" target="MARKER_LWTS"  weight="3.0"/>
      <Mapping source="LMT1" target="MARKER_LMT1"/>
      <Mapping source="LMT5" target="MARKER_LMT5"/>
      <Mapping source="LTOE" target="MARKER_LTOE"/>
